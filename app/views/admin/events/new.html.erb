<% provide(:custom_css, 'admin/events/form') %>

<section class="section-container">
  <div class="row">
    <div class="title">
      New Event
    </div>
    <div class="content">
      <div id="admin-form">
        <%= render 'form', event: @event, target_url: admin_events_path %>
      </div>

      <br><br>
      
      <div class="row">
        <div class="col-lg-12">
          <%= button_to("Back", admin_events_path, method: "get", class: "btn btn-info") %>
        </div>
      </div>
    </div>
  </div>
</section>