<section id="partners">
  <div class="container">
    <div class="section-header">
      <h2>Partners</h2>
    </div>

    <div class="row no-gutters partners-wrap clearfix justify-content-center wow fadeInUp">
      <% @partners.each do |partner| %>
        <div class="col-lg-3 col-md-4 col-xs-6">
          <div class="partner-logo">
            <%= image_tag(partner.logo, class: "img-fluid") %>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</section>